{
  "manifest_version": 2,

  "name": "breaker",
  "description": "Auto get the undo task",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    // "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "cookies",
    "clipboardRead",
    "clipboardWrite",
    "tabs",
    "*://*/*"
  ],
  "background":{
      // "scripts" : ["background.js"],
      "page"    : "background.html"
  },
  "options_page":"options.html",
  "content_scripts":[
    {
      "matches":["http://itry.com/*","http://r.ow.domob.cn/wx/ui*","http://ow.miidi.net/ow*","http://localhost:18080/itry*","http://m.qianka.com/*","http://m.shiwan123.com//*"],
      "js":[
            "sea-modules/seajs/2.2.0/sea-debug.js",
            "sea-modules/jquery/1.10.1/jquery-debug.js",
            "common/music.js",
            "common/paraMng.js",
            "common/redirect.js",
            "app/itry/hack.js",
            "app/itry/itry.js",
            "app/qianka/qianka.js",
            "app/domob/domob.js",
            "app/panda/panda.js",
            "app/reloadNotions.js",
            "content-script.js"
          ]
    }
  ]
}